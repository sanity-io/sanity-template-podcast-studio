{
  "version": 1,
  "title": "Podcast Studio",
  "description": "Manage your podcast with Sanity.io",
  "previewMedia": {
    "type": "image",
    "src": "assets/frontend.jpg",
    "alt": "A screenshot of the Podcast Studio."
  },
  "technologies": [
    {
      "id": "netlify",
      "name": "Netlify",
      "url": "https://www.netlify.com/"
    }
  ],
  "deployments": [
    {
      "id": "studio",
      "type": "studio",
      "title": "Sanity Studio",
      "description": "The Sanity Studio is where you edit and structure your content.",
      "dir": "./studio",
      "provider": {
        "name": "netlify",
        "config": {
          "base": "studio",
          "cmd": "npm run build && cp ./netlify.toml dist",
          "dir": "studio/dist"
        }
      },
      "previewMedia": {
        "type": "image",
        "src": "assets/studio-00.png",
        "alt": "A mock preview image"
      }
    }
  ]
}
